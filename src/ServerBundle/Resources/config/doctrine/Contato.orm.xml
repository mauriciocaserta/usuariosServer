<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ServerBundle\Entity\Contato" table="Contato">
    <id name="idusuario" type="integer" column="idUsuario">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomeusuario" type="string" column="nomeUsuario" length="45" nullable="false"/>
    <field name="emailusuario" type="string" column="emailUsuario" length="45" nullable="false"/>
    <field name="assuntousuario" type="string" column="assuntoUsuario" length="45" nullable="false"/>
    <field name="mensagemusuario" type="string" column="mensagemUsuario" length="255" nullable="false"/>
  </entity>
</doctrine-mapping>
